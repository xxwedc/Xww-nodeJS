/**app.wxss**/

/* 登录页面复用样式 */
@import 'css/login.wxss';

.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 
page,view,text,swiper,swiper-item,image,navigator{
  margin: 0;
  padding: 0;
  box-sizing: border-box;
}

page{
  --mainColor:#b67780;
}

/* 设置page宽高 */
page{
  width: 100%;
  height: 100%;
}