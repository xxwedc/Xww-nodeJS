/* componets/banner/banner.wxss */
.banner{
  width: 100%;
  height: 350rpx;
  border-radius: 20rpx;
  overflow: hidden;
  /* height: calc(118*750rpx/320); */
}
.swiper,.swiper-item,.image{
  width: 100%;
  height: 100%;
  border-radius: 20rpx;
}
.newsTitle{
  position: absolute;
  bottom: 10px;
  left: 10px;
  color: black;
  width: 250px;
  overflow: hidden;
  /* 结束后变为省略号 */
  text-overflow: ellipsis;
  /* 不换行 */
  white-space: nowrap;
}